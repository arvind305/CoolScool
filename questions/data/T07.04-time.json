  "topic_name": "Time",
  "canonical_explanation": {
    "text": "Time is measured in seconds, minutes, and hours. The 12-hour format uses AM and PM, while the 24-hour format uses 00:00 to 23:59. There are 60 seconds in a minute, 60 minutes in an hour, and 24 hours in a day.",
    "rules": [
      "1 hour = 60 minutes",
      "1 minute = 60 seconds",
      "1 day = 24 hours",
      "12-hour: AM (midnight to noon), PM (noon to midnight)",
      "24-hour: Add 12 for PM times after 12:00"
    ]
  },
