  "topic_id": "T07.06",
  "topic_name": "Money",
  "canonical_explanation": {
    "text": "Money in India is in Rupees (Rs) and Paise (p). 1 Rupee = 100 Paise. We can add, subtract, multiply, and divide amounts of money. Bills list items, quantities, rates, and totals. Profit = Selling Price - Cost Price (when SP > CP). Loss = Cost Price - Selling Price (when CP > SP).",
    "rules": ["1 Rupee = 100 Paise", "When adding/subtracting money, align the decimal points", "Bill format: Item, Quantity, Rate per unit, Amount", "Total = sum of all amounts in a bill", "Cost Price (CP): price at which an item is bought", "Selling Price (SP): price at which an item is sold", "Profit = SP - CP (when SP > CP)", "Loss = CP - SP (when CP > SP)", "No profit no loss when SP = CP"]
  },
  "questions": [
    {"question_id":"T07.06.Q001","concept_id":"T07.06.C01","difficulty":"familiarity","type":"mcq","question_text":"1 Rupee = ___ Paise","options":[{"id":"A","text":"10"},{"id":"B","text":"50"},{"id":"C","text":"100"},{"id":"D","text":"1000"}],"correct_answer":"C","explanation_correct":"1 Rupee = 100 Paise.","explanation_incorrect":"Rs 1 = 100 paise."},
    {"question_id":"T07.06.Q002","concept_id":"T07.06.C01","difficulty":"familiarity","type":"mcq","question_text":"Rs 5.50 means:","options":[{"id":"A","text":"5 rupees and 5 paise"},{"id":"B","text":"5 rupees and 50 paise"},{"id":"C","text":"55 rupees"},{"id":"D","text":"550 paise"}],"correct_answer":"B","explanation_correct":"Rs 5.50 = 5 rupees and 50 paise. The digits after the decimal point represent paise.","explanation_incorrect":"5.50 = Rs 5 and 50 paise."},
    {"question_id":"T07.06.Q003","concept_id":"T07.06.C01","difficulty":"familiarity","type":"mcq","question_text":"Rs 3.75 + Rs 2.25 = ?","options":[{"id":"A","text":"Rs 5.00"},{"id":"B","text":"Rs 6.00"},{"id":"C","text":"Rs 5.10"},{"id":"D","text":"Rs 5.50"}],"correct_answer":"B","explanation_correct":"Rs 3.75 + Rs 2.25 = Rs 6.00.","explanation_incorrect":"3.75 + 2.25 = 6.00."},
    {"question_id":"T07.06.Q004","concept_id":"T07.06.C01","difficulty":"familiarity","type":"mcq","question_text":"Rs 10.00 - Rs 4.50 = ?","options":[{"id":"A","text":"Rs 5.50"},{"id":"B","text":"Rs 6.50"},{"id":"C","text":"Rs 5.00"},{"id":"D","text":"Rs 4.50"}],"correct_answer":"A","explanation_correct":"Rs 10.00 - Rs 4.50 = Rs 5.50.","explanation_incorrect":"10.00 - 4.50 = 5.50."},
    {"question_id":"T07.06.Q005","concept_id":"T07.06.C01","difficulty":"application","type":"mcq","question_text":"Ravi has Rs 50. He buys a pencil for Rs 8.50 and an eraser for Rs 3.50. How much money is left?","options":[{"id":"A","text":"Rs 38.00"},{"id":"B","text":"Rs 42.00"},{"id":"C","text":"Rs 12.00"},{"id":"D","text":"Rs 48.00"}],"correct_answer":"A","explanation_correct":"Spent: Rs 8.50 + Rs 3.50 = Rs 12.00. Left: Rs 50 - Rs 12 = Rs 38.00.","explanation_incorrect":"8.50 + 3.50 = 12.00. 50 - 12 = Rs 38."},
    {"question_id":"T07.06.Q006","concept_id":"T07.06.C01","difficulty":"application","type":"mcq","question_text":"Cost of 5 notebooks at Rs 35 each:","options":[{"id":"A","text":"Rs 150"},{"id":"B","text":"Rs 175"},{"id":"C","text":"Rs 200"},{"id":"D","text":"Rs 140"}],"correct_answer":"B","explanation_correct":"5 × Rs 35 = Rs 175.","explanation_incorrect":"5 × 35 = Rs 175."},
    {"question_id":"T07.06.Q007","concept_id":"T07.06.C01","difficulty":"application","type":"mcq","question_text":"Anita bought 3 kg of sugar at Rs 42 per kg. Total cost:","options":[{"id":"A","text":"Rs 126"},{"id":"B","text":"Rs 84"},{"id":"C","text":"Rs 142"},{"id":"D","text":"Rs 136"}],"correct_answer":"A","explanation_correct":"3 × Rs 42 = Rs 126.","explanation_incorrect":"3 × 42 = Rs 126."},
    {"question_id":"T07.06.Q008","concept_id":"T07.06.C01","difficulty":"application","type":"mcq","question_text":"12 bananas cost Rs 60. Cost of each banana:","options":[{"id":"A","text":"Rs 4"},{"id":"B","text":"Rs 5"},{"id":"C","text":"Rs 6"},{"id":"D","text":"Rs 3"}],"correct_answer":"B","explanation_correct":"Rs 60 ÷ 12 = Rs 5 per banana.","explanation_incorrect":"60 ÷ 12 = Rs 5 each."},
    {"question_id":"T07.06.Q009","concept_id":"T07.06.C02","difficulty":"familiarity","type":"mcq","question_text":"A bill shows: 2 kg rice at Rs 45/kg and 1 kg dal at Rs 90/kg. The total amount is:","options":[{"id":"A","text":"Rs 135"},{"id":"B","text":"Rs 180"},{"id":"C","text":"Rs 225"},{"id":"D","text":"Rs 270"}],"correct_answer":"B","explanation_correct":"Rice: 2 × 45 = Rs 90. Dal: 1 × 90 = Rs 90. Total: Rs 90 + Rs 90 = Rs 180.","explanation_incorrect":"Rice = 2 × 45 = 90. Dal = 1 × 90 = 90. Total = Rs 180."},
    {"question_id":"T07.06.Q010","concept_id":"T07.06.C02","difficulty":"application","type":"mcq","question_text":"Sita's grocery bill: 3 kg potatoes at Rs 30/kg, 2 kg onions at Rs 25/kg, 1 kg tomatoes at Rs 40/kg. Total:","options":[{"id":"A","text":"Rs 170"},{"id":"B","text":"Rs 180"},{"id":"C","text":"Rs 175"},{"id":"D","text":"Rs 190"}],"correct_answer":"B","explanation_correct":"Potatoes: 3×30=90. Onions: 2×25=50. Tomatoes: 1×40=40. Total: 90+50+40=Rs 180.","explanation_incorrect":"90 + 50 + 40 = Rs 180."},
    {"question_id":"T07.06.Q011","concept_id":"T07.06.C03","difficulty":"familiarity","type":"mcq","question_text":"If Cost Price = Rs 50 and Selling Price = Rs 65, then:","options":[{"id":"A","text":"Profit = Rs 15"},{"id":"B","text":"Loss = Rs 15"},{"id":"C","text":"No profit no loss"},{"id":"D","text":"Profit = Rs 65"}],"correct_answer":"A","explanation_correct":"SP > CP, so Profit = SP - CP = 65 - 50 = Rs 15.","explanation_incorrect":"SP (65) > CP (50), so profit = 65 - 50 = Rs 15."},
    {"question_id":"T07.06.Q012","concept_id":"T07.06.C03","difficulty":"familiarity","type":"mcq","question_text":"If Cost Price = Rs 80 and Selling Price = Rs 60, then:","options":[{"id":"A","text":"Profit = Rs 20"},{"id":"B","text":"Loss = Rs 20"},{"id":"C","text":"No profit no loss"},{"id":"D","text":"Loss = Rs 60"}],"correct_answer":"B","explanation_correct":"CP > SP, so Loss = CP - SP = 80 - 60 = Rs 20.","explanation_incorrect":"CP (80) > SP (60), so loss = 80 - 60 = Rs 20."},
    {"question_id":"T07.06.Q013","concept_id":"T07.06.C03","difficulty":"familiarity","type":"true_false","question_text":"If the selling price equals the cost price, there is no profit and no loss.","correct_answer":"true","explanation_correct":"When SP = CP, the seller neither gains nor loses money.","explanation_incorrect":"SP = CP means no profit and no loss."},
    {"question_id":"T07.06.Q014","concept_id":"T07.06.C01","difficulty":"exam_style","type":"mcq","question_text":"Convert Rs 7.05 to paise:","options":[{"id":"A","text":"75 paise"},{"id":"B","text":"705 paise"},{"id":"C","text":"750 paise"},{"id":"D","text":"7050 paise"}],"correct_answer":"B","explanation_correct":"Rs 7.05 = 7 × 100 + 5 = 705 paise.","explanation_incorrect":"Rs 7.05 = 705 paise. (7 rupees = 700 paise + 5 paise = 705.)"},
    {"question_id":"T07.06.Q015","concept_id":"T07.06.C01","difficulty":"application","type":"fill_blank","question_text":"Rs 12.50 × 4 = Rs ___","correct_answer":"50.00","explanation_correct":"12.50 × 4 = 50.00.","explanation_incorrect":"12.50 × 4 = Rs 50.00."},
    {"question_id":"T07.06.Q016","concept_id":"T07.06.C02","difficulty":"application","type":"mcq","question_text":"Meera's stationery bill: 4 pens at Rs 15 each, 6 pencils at Rs 5 each, 2 erasers at Rs 3 each. Total:","options":[{"id":"A","text":"Rs 90"},{"id":"B","text":"Rs 96"},{"id":"C","text":"Rs 100"},{"id":"D","text":"Rs 86"}],"correct_answer":"B","explanation_correct":"Pens: 4×15=60. Pencils: 6×5=30. Erasers: 2×3=6. Total: 60+30+6=Rs 96.","explanation_incorrect":"60 + 30 + 6 = Rs 96."},
    {"question_id":"T07.06.Q017","concept_id":"T07.06.C02","difficulty":"exam_style","type":"mcq","question_text":"A shopkeeper's bill shows a total of Rs 560. The customer pays Rs 600. Change returned:","options":[{"id":"A","text":"Rs 40"},{"id":"B","text":"Rs 60"},{"id":"C","text":"Rs 50"},{"id":"D","text":"Rs 140"}],"correct_answer":"A","explanation_correct":"Change = Rs 600 - Rs 560 = Rs 40.","explanation_incorrect":"600 - 560 = Rs 40 change."},
    {"question_id":"T07.06.Q018","concept_id":"T07.06.C03","difficulty":"application","type":"mcq","question_text":"Ramesh bought a toy for Rs 120 and sold it for Rs 150. His profit is:","options":[{"id":"A","text":"Rs 20"},{"id":"B","text":"Rs 30"},{"id":"C","text":"Rs 270"},{"id":"D","text":"Rs 120"}],"correct_answer":"B","explanation_correct":"Profit = SP - CP = 150 - 120 = Rs 30.","explanation_incorrect":"Profit = 150 - 120 = Rs 30."},
    {"question_id":"T07.06.Q019","concept_id":"T07.06.C03","difficulty":"application","type":"mcq","question_text":"Kavitha bought a book for Rs 200 and sold it for Rs 175. She made a:","options":[{"id":"A","text":"Profit of Rs 25"},{"id":"B","text":"Loss of Rs 25"},{"id":"C","text":"Profit of Rs 375"},{"id":"D","text":"No profit no loss"}],"correct_answer":"B","explanation_correct":"CP (200) > SP (175). Loss = 200 - 175 = Rs 25.","explanation_incorrect":"SP < CP, so loss = 200 - 175 = Rs 25."},
    {"question_id":"T07.06.Q020","concept_id":"T07.06.C01","difficulty":"exam_style","type":"mcq","question_text":"Anu has a Rs 500 note. She buys items worth Rs 347.50. Change she gets:","options":[{"id":"A","text":"Rs 152.50"},{"id":"B","text":"Rs 153.50"},{"id":"C","text":"Rs 252.50"},{"id":"D","text":"Rs 162.50"}],"correct_answer":"A","explanation_correct":"Rs 500 - Rs 347.50 = Rs 152.50.","explanation_incorrect":"500 - 347.50 = Rs 152.50."},
    {"question_id":"T07.06.Q021","concept_id":"T07.06.C02","difficulty":"exam_style","type":"mcq","question_text":"Prepare a bill: 5 kg wheat flour at Rs 32/kg, 2 kg sugar at Rs 40/kg, 1 kg ghee at Rs 450/kg. Grand total:","options":[{"id":"A","text":"Rs 690"},{"id":"B","text":"Rs 700"},{"id":"C","text":"Rs 710"},{"id":"D","text":"Rs 730"}],"correct_answer":"A","explanation_correct":"Wheat flour: 5×32=160. Sugar: 2×40=80. Ghee: 1×450=450. Total: 160+80+450=Rs 690.","explanation_incorrect":"160 + 80 + 450 = Rs 690."},
    {"question_id":"T07.06.Q022","concept_id":"T07.06.C03","difficulty":"exam_style","type":"mcq","question_text":"A shopkeeper bought 10 pens at Rs 8 each and sold them at Rs 12 each. Total profit:","options":[{"id":"A","text":"Rs 20"},{"id":"B","text":"Rs 40"},{"id":"C","text":"Rs 80"},{"id":"D","text":"Rs 120"}],"correct_answer":"B","explanation_correct":"CP total = 10 × 8 = Rs 80. SP total = 10 × 12 = Rs 120. Profit = 120 - 80 = Rs 40.","explanation_incorrect":"Profit per pen = 12 - 8 = Rs 4. Total = 10 × 4 = Rs 40."},
    {"question_id":"T07.06.Q023","concept_id":"T07.06.C01","difficulty":"familiarity","type":"mcq","question_text":"350 paise = ?","options":[{"id":"A","text":"Rs 3.50"},{"id":"B","text":"Rs 35"},{"id":"C","text":"Rs 0.35"},{"id":"D","text":"Rs 350"}],"correct_answer":"A","explanation_correct":"350 paise ÷ 100 = Rs 3.50.","explanation_incorrect":"350 paise = 350 ÷ 100 = Rs 3.50."},
