{
  "rewrites": [
    { "source": "/", "destination": "/ui/index.html" },
    { "source": "/css/(.*)", "destination": "/ui/css/$1" },
    { "source": "/js/(.*)", "destination": "/ui/js/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
